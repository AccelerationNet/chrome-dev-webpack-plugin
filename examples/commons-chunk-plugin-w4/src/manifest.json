{
  "manifest_version": 2,
  "description": "Tests the capacity to properly patch the manifest.json when using the webpack.optimize.SplitChunksPlugin plugin",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    },
    {
      "matches": ["*://*/*"],
      "js": ["pages/index.js", "pages/page1.js", "pages/page2.js"]
    }
  ]
}
